{
	"docsRoot": "./docs",
	"entryPoints": ["packages/*/"],
	"exclude": [
		"packages/ai-orchestrator/",
		"packages/demo/",
		"packages/template/"
	],
	"entryPointStrategy": "packages",
	"excludeExternals": true,
	"externalPattern": ["**/node_modules/**"],
	"excludePrivate": true,
	"githubPages": true,
	"hideBreadcrumbs": true,
	"hidePageHeader": true,
	"out": "docs/packages/",
	"plugin": [
		"typedoc-plugin-markdown",
		// // custom plugin to add "try" button to examples
		// "./scripts/auto-try-buttons.js",
		// "./scripts/debug-hooks.js",
		"typedoc-vitepress-theme"
	],
	// "readme": "packages/README.md",
	/* t4ee */
	"pretty": true
}
